{
  "expo": {
    "name": "aung-zay",
    "slug": "aung-zay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.tinmyowin.aungzay"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "babel": {
        "include": [
          "@ui-kitten/components"
        ]
      }
    },
    "extra": {
      "eas": {
        "projectId": "2817bbc1-214b-4883-a510-0aa0a23fa324"
      }
    }
  }
}
